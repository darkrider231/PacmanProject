ull,"original":{"line":190,"column":0},"generated":{"line":190,"column":0}}],"sources":{"style.css":"* {\n  box-sizing: border-box;\n  font-family: 'Press Start 2P', cursive;\n}\n\nbody {\n  background: url('background.jpg') no-repeat center center fixed;\n  background-size: cover;\n}\n\n#wrapper {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n}\n\n#game {\n  position: relative;\n  display: inline-grid;\n  background: black;\n}\n\n.game-status {\n  position: absolute;\n  top: 0;\n  left: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  height: 50px;\n  border: 5px red solid;\n  background: black;\n  color: white;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 1.5rem;\n}\n\n#score {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 200px;\n  height: 50px;\n  border: 5px red solid;\n  background: black;\n  margin-top: 2px;\n  color: white;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 2rem;\n  margin-bottom: 5px;\n  margin-left: -700px;\n}\n\n#start-button {\n  margin-top: 20px;\n  font-size: 20px;\n  background: red;\n  color: aliceblue;\n  border: none;\n  padding: 5px;\n  border-radius: 4px;\n}\n\n.hide {\n  display: none;\n}\n\n.square {\n  display: block;\n}\n\n.blank {\n  background: black;\n}\n\n.wall {\n  border: 1px solid red;\n}\n\n.dot {\n  background: white;\n  border: 8px solid black;\n  border-radius: 50%;\n}\n\n.pill {\n  background:yellow;\n  border: 5px solid black;\n  border-radius: 50%;\n}\n\n.ghost {\n  position: relative;\n  border-radius: 10px 10px 2px 2px;\n  border: none;\n}\n\n.ghost::before {\n  content: '';\n  display: block;\n  position: absolute;\n  width: 3px;\n  height: 3px;\n  background: black;\n  border-radius: 50%;\n  left: 3px;\n  top: 5px;\n  border: 2px solid white;\n}\n\n.ghost::after {\n  content: '';\n  display: block;\n  position: absolute;\n  width: 3px;\n  height: 3px;\n  background: black;\n  border-radius: 50%;\n  right: 3px;\n  top: 5px;\n  border: 2px solid white;\n}\n\n.scared {\n  background: blue !important;\n}\n\n.blinky {\n  background: red;\n}\n\n.pinky {\n  background: pink;\n}\n\n.inky {\n  background: cyan;\n}\n\n.clyde {\n  background: orange;\n}\n\n.pacman {\n  background: yellow;\n  border: none;\n  border-radius: 50%;\n  clip-path: polygon(\n    0 0,\n    20px 0,\n    20px -5px,\n    8px 10px,\n    20px 25px,\n    20px 20px,\n    0 20px\n  );\n  animation: chew 0.2s linear forwards infinite;\n}\n\n@keyframes chew {\n  50% {\n    clip-path: polygon(\n      0 0,\n      20px 0,\n      20px 10px,\n      8px 10px,\n      20px 10px,\n      20px 20px,\n      0 20px\n    );\n  }\n}\n\n/* My Styles */\nh1 {\n  color: aliceblue;\n  font-size:x-large;\n  text-align: center;\n}\n\n#oneUp{\n  color: aliceblue;\n  margin-left: -700px;\n  font-size: 25px;\n  margin-bottom: 0px;\n  font-weight: bold;\n}"},"lineCount":null}},"error":null,"hash":"6cff40e69ec16321686ef9272e6091b5","cacheData":{"env":{}}}